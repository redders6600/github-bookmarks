{
  "name": "Github saved issue searches (alpha)",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This is a minimal extension that allows you to save issue searches on github by adding to the existing UI.",
  "homepage_url": "https://github.com/redders6600/github-bookmarks",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://github.com/*",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}
